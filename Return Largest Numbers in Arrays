function largestOfFour(arr) {
  // You can do this!
  
  var newArr = [];
  
  for(var k=0; k<arr.length; k++){
    var largest = 0;
    for(var i = 0; i < arr.length; i++){
      if(arr[k][i] > largest){
        largest = arr[k][i];
        
        newArr[k] = largest;
        console.log(newArr);
      }
    }
    
  }
  //console.log(newArr);
  return newArr;
}

largestOfFour([[13, 27, 18, 26], [4, 5, 1, 3], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
